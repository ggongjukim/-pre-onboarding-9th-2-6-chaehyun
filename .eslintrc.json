{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-var": "error",
    "comma-dangle": ["off", { "functions": "ignore" }],
    "func-style": ["error", "expression"],
    "react/function-component-definition": [2, { "namedcomponents": "arrow-function" }],
    "linebreak-style": 0
  }
}
